{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "stylelint.validate": ["css", "scss", "vue", "html"],
  "eslint.validate": ["typescript", "vue", "html", "json"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "json.format.enable": false,
  "files.associations": {
    "*.json": "jsonc"
  },
  "files.eol": "\n",
  "cSpell.words": [
    "aliapp",
    "alipay",
    "alipays",
    "autoclose",
    "autocrlf",
    "autofocus",
    "automator",
    "automerge",
    "browserslist",
    "builtins",
    "camelcase",
    "channing",
    "commitlint",
    "dbaeumer",
    "dcloudio",
    "degit",
    "doweidu",
    "eqeqeq",
    "errmsg",
    "esbenp",
    "eslintignore",
    "FILESYSTEMS",
    "gitee",
    "glup",
    "haoshiqi",
    "initconfig",
    "johnsoncodehk",
    "juejin",
    "kuaishou",
    "lintstagedrc",
    "miniapp",
    "monokai",
    "nvue",
    "Persistedstate",
    "pinia",
    "Prefinem",
    "prettierignore",
    "quickapp",
    "riskgo",
    "scrolltolower",
    "sheight",
    "smartblue",
    "solt",
    "splashscreen",
    "Strat",
    "stylelint",
    "stylelintrc",
    "swidth",
    "toutiao",
    "uniapp",
    "unocss",
    "unplugin",
    "vite",
    "vuedx",
    "vuex",
    "weixin"
  ]
}
